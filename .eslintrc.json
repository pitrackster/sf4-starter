{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": { }
  },
  "rules": {
    "indent": [2, 2, { "SwitchCase": 1 }],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never"
    }]
  },
  "globals": {
    "$": false,
    "Routing": false,
    "Toastr" : false,
    "Translator": false,
    "__dirname": false,
    "module": false,
    "require": false,
    "process": false
  }
}
